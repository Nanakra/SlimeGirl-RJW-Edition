<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<BodyDef>
		<defName>Slime</defName>
		<label>slime</label>
		<corePart>
			<def>Torso</def>
			<height>Middle</height>
			<depth>Outside</depth>
			<groups>
				<li>Torso</li>
			</groups>
			<parts>
				<li>					
					<def>SlimeHead</def>
					<coverage>0.20</coverage>
					<depth>Outside</depth>
					<height>Top</height>
					<groups>
						<li>UpperHead</li>
						<li>FullHead</li>
						<li>HeadAttackTool</li>
					</groups>
				</li>
				<li>
					<def>Chest</def>
					<coverage>0.1</coverage>
					<height>Top</height>
					<depth>Outside</depth>
					<groups>
						<li>Torso</li>
					</groups>
				</li>
				<li>
					<def>SlimeTentacle</def>
					<customLabel>left tentacle</customLabel>
					<coverage>0.15</coverage>
					<depth>Outside</depth>
					<height>Middle</height>
					<groups>
						<li>Arms</li>
					</groups>
				</li>
				<li>
					<def>SlimeTentacle</def>
					<customLabel>right tentacle</customLabel>
					<coverage>0.15</coverage>
					<depth>Outside</depth>
					<height>Middle</height>
					<groups>
						<li>Arms</li>
					</groups>
				</li>
				<li>
					<def>SlimeWaist</def>
					<customLabel>waist</customLabel>
					<coverage>0</coverage>
					<height>Bottom</height>
					<depth>Outside</depth>
					<groups>
						<li>Waist</li>
					</groups>
				</li>
				<li>
					<def>SlimeLowerBody</def>
					<customLabel>lower body</customLabel>
					<coverage>0.20</coverage>
					<height>Bottom</height>
					<depth>Outside</depth>
					<groups>
						<li>Torso</li>
					</groups>
				</li>
				<li>
					<def>SlimeCore</def>
					<coverage>0.0001</coverage>
					<depth>Inside</depth>
					<height>Bottom</height>
					<groups>
						<li>Waist</li>
					</groups>
				</li>
			</parts>
		</corePart>
	</BodyDef>


</Defs>
